inherit rubygem

NAME="ruby-uglifier"
VERSION=2.5.3
RELEASE=1
CATEGORY="Ruby"
SUMMARY="Ruby JavaScript compressor extension"
DESCRIPTION="Ruby wrapper for UglifyJS JavaScript compressor"

ARCH=noarch

PKG_NAMES="${NAME} ${NAME}-doc"
